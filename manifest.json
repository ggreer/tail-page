{
  "name": "Tail Page",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Stay scrolled to the bottom of a page. Handy for buildbot output.",
  "icons": {
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_icon": "icons/icon.png"
  },
  "background": {"scripts": ["background.js"]},
  "options_page": "options.html",
  "permissions": ["tabs", "<all_urls>"]
}
